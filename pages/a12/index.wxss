/************** item1 *****************/
/* 中间竖线 */
.box_item_r_1:after {
  content: "";
  margin-left: 10px;
  padding-left: 10px;
  border-left: 1px solid red;
  height: 32rpx;
  display: inline-block;
  vertical-align: middle;
  margin-right: 10rpx;
}
/* 箭头 */
/* 实心 */
.arrow_01::after {
  border: 10px solid transparent;
  border-left: 10px solid #f00;
  width: 0;
  height: 0;
  position: absolute;
  content: ' ';
  transform: rotate(90deg);
}
/* 空心 */
/* 上 */
.arrow_1 {
  width: 16rpx;
  height: 16rpx;
  border-top: 2rpx solid red;
  border-left: 2rpx solid red;
  transform: rotate(45deg);
}
/************** item2 *****************/
/* 对号 */
.success{
  width: 10rpx;
  height: 24rpx;
  border-color: #009933;
  border-style: solid;
  border-width: 0 4rpx 6rpx 0;
  transform: rotate(45deg);
}
/* 叉号 */
.error {
  width: 10rpx;
  height: 10rpx;
  margin: auto;
  position: relative;
}
.error::before,
.error::after {
  content: "";
  position: absolute;
  height: 24rpx;
  width: 1px;
  top: 0px;
 background-color: red;
}
.error::before {
  transform: rotate(45deg);
}
.error::after {
  transform: rotate(-45deg);
}
/* 圆点 */
.dian {
  position: relative;
  padding: 0 10rpx;
}
.dian::after {
  content: '';
  position: absolute;
  width: 10rpx;
  height: 10rpx;
  background: #f06406;
  left: 8rpx;
  top: 18rpx;
  border-radius: 50%;
}
