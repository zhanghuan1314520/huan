/* 中间竖线 */
.box_item_r_1:after {
  content: "";
  margin-left: 10px;
  padding-left: 10px;
  border-left: 1px solid red;
  height: 32rpx;
  display: inline-block;
  vertical-align: middle;
  margin-right: 10rpx;
}

/* 箭头 */
/* 实心 */
.arrow_01::after {
  border: 10px solid transparent;
  border-left: 10px solid #f00;
  width: 0;
  height: 0;
  position: absolute;
  content: ' ';
  transform: rotate(90deg);
}

/* 空心 */
/* 上 */
.arrow_1 {
  width: 16rpx;
  height: 16rpx;
  border-top: 2rpx solid red;
  border-left: 2rpx solid red;
  transform: rotate(45deg);
}

.dian {
  position: relative;
  padding: 0 10rpx;
}

.dian::after {
  content: '';
  position: absolute;
  width: 10rpx;
  height: 10rpx;
  background: #f06406;
  left: 8rpx;
  top: 18rpx;
  border-radius: 50%;
}

/* item3 */
.btm {
  width: 100%;
  font-size: 30rpx;
}

.btm .tit-wrap {
  width: 100%;
  padding: 4rpx 15rpx 10rpx 0rpx;
  box-sizing: border-box;
  margin-top: 16rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.btm .tit-wrap .txt {
  font-size: 30rpx;
}

.btm .tit-wrap .arrow_a {
  width: 16rpx;
  height: 16rpx;
  border-top: 2rpx solid #fff;
  border-left: 2rpx solid #fff;
  transform: rotate(-135deg);
  margin-top: -10rpx;
  margin-right: 4rpx;
}

.btm .tit-wrap .arrow_a.active {
  margin-top: 8rpx;
  transform: rotate(45deg);
}

.btm .plot {
  text-indent: 2em;
}

/* item4 */
.content {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  text-overflow: ellipsis;
  overflow: hidden;
  font-size: 28rpx;
}

.ellipsis_1 {
  text-align: center;
  margin-top: 10rpx;
  font-size: 28rpx;
  color: #f00;
}

/* 修改列数 */
.ellipsis {
  -webkit-line-clamp: 3;
}

/* 展示全部 */
.unellipsis {
  -webkit-line-clamp: 0;
}


.btm .tit-wrap .arrow_b {
  width: 16rpx;
  height: 16rpx;
  border-top: 2rpx solid #fff;
  border-left: 2rpx solid #fff;
  transform: rotate(-135deg);
  margin-top: -10rpx;
  margin-right: 4rpx;
}

.btm .tit-wrap .arrow_b.active {
  margin-top: 8rpx;
  transform: rotate(45deg);
}

/* item6 */
/* 单行文本超出 */
.team-content {
  width: 500rpx;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
  word-break: break-all;
}

/* 多行文本超出 */
.homework {
  display: -webkit-box;
  /*设置为弹性盒子*/
  -webkit-line-clamp: 2;
  /*最多显示5行*/
  overflow: hidden;
  /*超出隐藏*/
  text-overflow: ellipsis;
  /*超出显示为省略号*/
  -webkit-box-orient: vertical;
  word-break: break-all;
  /*强制英文单词自动换行*/
}

